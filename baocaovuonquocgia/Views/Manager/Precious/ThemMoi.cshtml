
<ul class="nav nav-tabs nav-tabs-colored">
    <li><a onclick="ChuyenTab('ChuyenTabDanhSach')">Danh sách</a></li>
    <li class="active"><a>Thêm mới</a></li>
</ul>
<div class="tab-content tab-content-colored">
    <div class="tab-pane fade in active">
        <div class="panel-content">
            <form novalidate id="form_ThemmoiQuy">
                <div class="row">
                    <div class="col-md-6">
                        <div class="form-group">
                            <label for="txt_precious_name">Tên (Ex: Quý I - 20....) <required> * </required></label>
                            <input type="text" id="txt_precious_name" name="precious_name" class="form-control" onkeyup="ElementValidation(this, [{ name: 'rangelength', requirement: [5,100] }])">
                            <label class="parsley-required"></label>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="form-group">
                            <label for="txt_completedate">Hạn hoàn thành <required> * </required></label>
                            <input type="text" id="txt_completedate" name="completedate" class="form-control" data-format="DD-MM-YYYY" data-template="DD - MM - YYYY">
                            <label class="parsley-required"></label>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-6">
                        <div>
                            <label for="startdate">Ngày báo cáo <required> * </required></label><br />
                            <div class="daterange">
                                <input type="text" id="txt_startdate" name="startdate" class="form-control" data-format="DD-MM-YYYY" data-template="DD - MM - YYYY"> đến
                                <input type="text" id="txt_enddate" name="enddate" class="form-control" data-format="DD-MM-YYYY" data-template="DD - MM - YYYY">
                            </div>

                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="form-group">
                            <label for="txt_slowdate">Ngày báo chậm <required> * </required></label>
                            <input type="text" id="txt_slowdate" name="slowdate" class="form-control" data-format="DD-MM-YYYY" data-template="DD - MM - YYYY">
                            <label class="parsley-required"></label>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <div class="form-group">
                            <button type="button" class="btn btn-primary float-right" id="btn_themmoi">Thêm mới</button>
                            <button type="button" class="btn btn-primary float-right" id="btn_reset">Làm lại</button>
                        </div>
                    </div>
                </div>

            </form>
        </div>
    </div>
</div>

