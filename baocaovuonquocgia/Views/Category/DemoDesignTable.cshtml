
@{
    ViewBag.Title = "DemoDesignTable";
    Layout = null;
}
<style type="text/css">
    input.value {
        padding: 5px;
        border: 1px solid #999;
        vertical-align: middle;
        margin-left: 2px;
    }
    .button-group, .button-group li {
        display: inline-block;
        zoom: 1;
        vertical-align: middle;
    }
    #initialize, #merge {
        margin-left: 20px;
    }
    .button {
        display: inline-block;
        zoom: 1;
        padding: 6px 10px;
        margin: 0;
        cursor: pointer;
        border: 1px solid #bbb;
        overflow: visible;
        font: 700 13px arial,helvetica,sans-serif;
        text-decoration: none;
        white-space: nowrap;
        color: #555;
        line-height: 1;
        background-color: #ddd;
        background-image: -webkit-gradient(linear,left top,left bottom,from(rgba(255,255,255,1)),to(rgba(255,255,255,0))),url(data:image/png;base64,);
        background-image: -webkit-linear-gradient(top,rgba(255,255,255,1),rgba(255,255,255,0)),url(data:image/png;base64,);
        background-image: -moz-linear-gradient(top,rgba(255,255,255,1),rgba(255,255,255,0)),url(data:image/png;base64,);
        background-image: -ms-linear-gradient(top,rgba(255,255,255,1),rgba(255,255,255,0)),url(data:image/png;base64,);
        background-image: -o-linear-gradient(top,rgba(255,255,255,1),rgba(255,255,255,0)),url(data:image/png;base64,);
        background-image: linear-gradient(top,rgba(255,255,255,1),rgba(255,255,255,0)),url(data:image/png;base64,);
        transition: background-color .2s ease-out;
        background-clip: padding-box;
        -moz-box-shadow: 0 1px 0 rgba(0,0,0,.3),0 2px 2px -1px rgba(0,0,0,.5),0 1px 0 rgba(255,255,255,.3) inset;
        -webkit-box-shadow: 0 1px 0 rgba(0,0,0,.3), 0 2px 2px -1px rgba(0,0,0,.5), 0 1px 0 rgba(255,255,255,.3) inset;
        box-shadow: 0 1px 0 rgba(0,0,0,.3), 0 2px 2px -1px rgba(0,0,0,.5), 0 1px 0 rgba(255,255,255,.3) inset;
        text-shadow: 0 1px 0 rgba(255,255,255,.9);
    }
    .button-group .button {
        font-size: 13px;
        -moz-border-radius: 0;
        -webkit-border-radius: 0;
        border-radius: 0;
        border-width: 1px 1px 1px 0;
    }
    .button-group li.first .button {
        -moz-border-radius: 3px 0 0 3px;
        -webkit-border-radius: 3px 0 0 3px;
        border-width: 1px;
    }
    .group-colrow {
        display: inline-block;
        font-weight:bold
    }
    .main-design {
        border: solid #f6f6f6;
        border-width: 4px 0;
        display: block;
        width: 100%;
        background-color: #fff;
        height: 500px;
        overflow-y:scroll
    }

    table.ui-selectable th {
        padding: 3px;
        width: 30px;
        height: 25px;
    }
    table.ui-selectable, table.ui-selectable th {
        border: 1px solid #595959;
        border-collapse: collapse;
        border-spacing: 2px;
        text-align: center;
    }
    table.ui-selectable {
        margin: 0 auto;
        cursor: crosshair;
    }
        table.ui-selectable th.ui-selected {
            background: #ff8080;
        }
    .button.btn-enable {
        background: #c7c7c7;
    }
</style>
<div class="designTable">
    <div class="group-colrow">
        row
        <input class="value" type="number" name="row" value="3" min="1" style="width:50px">
        x col
        <input class="value" type="number" name="col" value="3" min="1" style="width:50px">
    </div>
    <ul class="button-group">
        <li class="first">
            <button id="merge" title="Gộp dòng hoặc cột" class="button">Gộp lại</button>
        </li>
        <li>
            <button id="split_hor" title="Chia đôi theo hàng ngang" class="button">Chia dòng</button>
        </li>
        <li>
            <button id="split_ver" title="Chia đôi theo hàng dọc" class="button">Chia cột</button>
        </li>
        <li>
            <button id="replace" title="Replace <td> and <th> each other" class="button">td ↔ th</button>
        </li>
        <li>
            <button id="chars" title="Enter the character(s)" class="button">Characters</button>
        </li>
        <li>
            <button id="output-chars" title="Output the character(s)" class="button" style="display: none;">Output</button>
        </li>
        <li>
            <button id="SelectCell" title="Lựa chọn ô" class="button">Lựa chọn ô</button>
        </li>
        <li>
            <button id="ClearProcess" title="Xóa chức năng đã chọn" class="button">Xóa chức năng</button>
        </li>
        <li class="last">
            <button id="ClearCellSelected" title="Xóa ô đã chọn" class="button">Xóa ô</button>
        </li>
    </ul>
    <div class="main-design">
        <table class="ui-selectable">
            <thead>
                
            </thead>
        </table>
    </div>
</div>
<script type="text/javascript">
    var EventCurrent;
    var isMouseDown = false;
    var startRowIndex = null;
    var startCellIndex = null;
    var table;
    var startX = 0, startY = 0;
    var endX = 0, endY = 0;
    document.addEventListener("DOMContentLoaded", function (event) {
        table = document.querySelector('table.ui-selectable')
        InitNew();

        //table.addEventListener('mousedown', function (e) {
        //    startX = e.clientX;
        //    startY = e.clientY;
        //    isMouseDown = true;
        //    return false;
        //})
        //table.addEventListener('mouseover', function () {
        //    if (!isMouseDown) return;
        //})
        //table.addEventListener('selectstart', function () {
        //    return false;
        //})
        document.getElementById('split_hor').addEventListener('click', function () {
            this.classList.add('btn-enable');
            EventCurrent = 'SPLIT_HOR';
            removeClassBtnEnable(this);
            EventLastProcess();
        })
        document.getElementById('merge').addEventListener('click', function () {
            this.classList.add('btn-enable');
            EventCurrent = 'MEGER';
            removeClassBtnEnable(this);
            EventLastProcess();
        })
        document.getElementById('split_ver').addEventListener('click', function () {
            this.classList.add('btn-enable');
            EventCurrent = 'SPLIT_VER';
            EventLastProcess();
        })
        document.getElementById('split_ver').addEventListener('click', function () {
            this.classList.add('btn-enable');
            EventCurrent = 'SPLIT_VER';
            EventLastProcess();
        })
        document.getElementById('ClearProcess').addEventListener('click', function () {
            this.classList.add('btn-enable');
            EventCurrent = undefined
            removeClassBtnEnable(this)
        })
        document.getElementById('ClearCellSelected').addEventListener('click', function () {
            
            removeClassThSelected()
        })
        document.getElementById('SelectCell').addEventListener('click', function () {
            EventCurrent = 'SELECT_CELL';
            this.classList.add('btn-enable');
            
            removeClassBtnEnable(this)
            removeClassThSelected()
        })
    });
    document.onmouseup = function (e) {
        endX = e.clientX;
        endY = e.clientY;
        selectCellByMouse();
        isMouseDown = false;
        console.log('mouseup',isMouseDown)
    };
    function selectCellByMouse() {
        if (EventCurrent == 'SELECT_CELL') {
            var RectB = {
                left: startX,
                top: startY,
                right: endX,
                bottom: endY
            }
            var arrayTh = Array.from(table.querySelectorAll('thead tr th'));
            var minX = Math.min.apply(Math, arrayTh.map(function (o) { return o.getBoundingClientRect().x; }));
            var minY = Math.min.apply(Math, arrayTh.map(function (o) { return o.getBoundingClientRect().y; }));
            var maxX = Math.max.apply(Math, arrayTh.map(function (o) { return o.getBoundingClientRect().x; }));
            var maxY = Math.max.apply(Math, arrayTh.map(function (o) { return o.getBoundingClientRect().x; }));
            for (var i = 0; i < arrayTh.length; i++) {
                var th = arrayTh[i];
                var Rect = th.getBoundingClientRect();
                var RectA = {
                    left: Rect.left,
                    top: Rect.top,
                    right: Rect.right,
                    bottom: Rect.bottom
                }
                
                if (intersectRect(RectA, RectB)) {
                    th.classList.add('ui-selected');
                }
            }
            startX = 0, startY = 0, endX = 0, endY = 0;
        }
        
    }
    function intersectRect(r1, r2) {
        return !(r2.left > r1.right ||
            r2.right < r1.left ||
            r2.top > r1.bottom ||
            r2.bottom < r1.top);
    }

    function removeClassBtnEnable($btn) {
        Array.from(document.querySelectorAll('.button-group button.button')).forEach(function (btn) {
            if ($btn != btn) {
                btn.classList.remove('btn-enable');
            }
        });
    }
    function removeClassThSelected($th) {
        Array.from(document.querySelectorAll('table.ui-selectable th.ui-selected')).forEach(function (th) {
            if ($th != th) {
                th.classList.remove('ui-selected');
            }
        });
    }
    function InitNew() {
        var $table = document.querySelector('.main-design table');
        for (var i = 0; i < 3; i++) {
            var $tr = createTr();
            for (var j = 0; j < 3; j++) {
                var $th = createTh();
                $tr.appendChild($th);
            }
            $table.querySelector('thead').appendChild($tr);
        }
    }



    function createTr() {
        var $tr = document.createElement('tr');
        return $tr;
    }

    function createTh($option) {
        var $th = document.createElement('th');
        if ($option && $option.length > 0) {
            for (var i = 0; i < $option.length; i++) {
                var $op = $option[i];
                $th.setAttribute($op.name,$op.value)
            }
        }
        $th.onmousedown= function (e) {
            if (isMouseDown == false) {
                startX = e.clientX;
                startY = e.clientY;
                removeClassThSelected();
            }
            isMouseDown = true;
            return false;
        }
        $th.addEventListener('mouseover', function () {
            if (!isMouseDown) return;
        })
        $th.addEventListener('selectstart', function () {
            return false;
        })
        return $th;
    }

    function EventLastProcess() {
        var $table = document.querySelector('table.ui-selectable');
        var $arrThSelected = Array.from(document.querySelectorAll('table.ui-selectable th.ui-selected'));
        switch (EventCurrent) {
            case 'SPLIT_HOR':
                if ($arrThSelected.length == 1 && $arrThSelected[0].rowSpan>1) {
                    
                    EventCurrent = undefined;
                    removeClassBtnEnable();
                }
                break;
            case 'SPLIT_VER':
                if ($arrThSelected.length == 1) {
                    
                    var $thNew = createTh([
                        { name: 'class', value: 'ui-selected' },
                        { name: 'rowSpan', value: $arrThSelected[0].rowSpan }
                    ]);
                    insertAfter($thNew, $arrThSelected[0]);
                    var Index = Array.from($arrThSelected[0].closest('tr').querySelectorAll('th')).indexOf($arrThSelected[0])
                    Index = 0;
                    for (var i = 0; i < $arrThSelected[0].closest('tr').querySelectorAll('th').length; i++) {
                        var th = $arrThSelected[0].closest('tr').querySelectorAll('th')[i];
                        if ($arrThSelected[0] == th) {
                            Index++;
                            break;
                        }
                        else {
                            Index += th.colSpan;
                        }
                    }
                    Array.from($arrThSelected[0].closest('table').querySelectorAll('thead tr')).forEach(function (tr) {
                        if ($arrThSelected[0].closest('tr') !=tr && tr.querySelectorAll('th')[Index]) {
                            tr.querySelectorAll('th')[Index].colSpan = tr.querySelectorAll('th')[Index].colSpan + 1;
                        }
                    });
                    EventCurrent = undefined;
                    removeClassBtnEnable();
                }
                break;
            case 'MEGER':
                if ($arrThSelected.length > 1) {
                    var thFirst = $arrThSelected[0];
                    var ColSpan = -1, RowSpan = 1;
                    var arrRow = [];
                    for (var i = 0; i < $arrThSelected.length; i++) {
                        var $row = $arrThSelected[i].closest('tr');
                        if (arrRow.indexOf($row) == -1) {
                            arrRow.push($row);
                            if (ColSpan == -1) {
                                ColSpan = 1;
                            }
                        }
                        else {
                            if (arrRow.length == 1) {
                                ColSpan++;
                            }
                        }
                    }
                    thFirst.colSpan = ColSpan;
                    thFirst.rowSpan = arrRow.length;
                    for (var i = 0; i < $arrThSelected.length; i++) {
                        if (i > 0)
                            $arrThSelected[i].remove();
                    }
                }
                EventCurrent = undefined;
                removeClassBtnEnable();
                break;
        }
    }
    function insertAfter(newNode, referenceNode) {
        referenceNode.parentNode.insertBefore(newNode, referenceNode.nextSibling);
    }

</script>


